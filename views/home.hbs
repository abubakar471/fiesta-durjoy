<body>
    {{> default_header}}
    <div class="home-banner">
        <div>
            <div class="div-1">
                <h1>Share Your Moments</h1>
            </div>
            <div class="div-2">
                <a href="">Get VIP Membership</a>
                <a href="">Subscribe for Newsletter</a>
            </div>
        </div>
    </div>


    <section class="home-posts-container">
        <div class="home-all-posts">
            {{#each posts}}

            <a href="/posts/{{this._id}}">

                <div class="profile-posts-card">
                    {{#if postPhoto}}

                    <div class="auth-profile-postPhoto" style="display: flex; ">

                        <p>
                            <img src="{{postPhoto.[0].url}}" class="auth-profile-postPhoto-image"></img>
                        </p>


                    </div>
                    {{else}}

                    <div class="auth-profile-postPhoto" style="display: flex; ">
                        <p>
                            <video controls>
                                <source src="{{postVideo.[0].url}}" type="video/mp4">
                            </video>
                        </p>
                    </div>

                    {{/if}}


                </div>
            </a>

            {{/each}}
        </div>
    </section>


    <div class="featured-posts-see-more-div">
        <a href=""><img src="/images/image5.png" alt=""></a>
        <a href="/all-posts" class="featured-posts-see-more">See More</a>
    </div>

    <section class="home-middle-banner">
